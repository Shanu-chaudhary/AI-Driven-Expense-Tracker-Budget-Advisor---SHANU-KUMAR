# Copy this to `application-secret.properties` (gitignored) and fill with real secrets.
# Do NOT commit the real file.

# JWT secret (long random string)
jwt.secret=CHANGE_ME_TO_A_LONG_RANDOM_SECRET

# Optional: override expiration (milliseconds)
jwt.expiration=3600000

# Mail credentials
spring.mail.username=you@example.com
spring.mail.password=your-email-password

# Optional Mongo override
spring.data.mongodb.uri=mongodb://localhost:27017/mydb
